{
    "openai_api_key": "",
    "openrouter_api_key": "",
    "model": "gpt-5",
    "temperature": 0.7,
    "workspace": "./workspace",
    "llm_provider": "openai",
    "local_model_name": "microsoft/phi-2",
    "device": "cpu",
    "enable_multi_agent": false,
    "specialized_agents": [
      {
        "name": "リサーチャー",
        "role": "研究者",
        "expertise": ["データ分析", "情報検索", "文献調査"],
        "model_name": "gpt-5",
        "temperature": 0.5,
        "provider": "openai"
      },
      {
        "name": "クリティカルシンカー",
        "role": "批判的思考家",
        "expertise": ["論理分析", "仮説検証", "反論提示"],
        "model_name": "gpt-5",
        "temperature": 0.7,
        "provider": "openai"
      },
      {
        "name": "クリエイティブソルバー",
        "role": "創造的問題解決者",
        "expertise": ["アイデア生成", "創造的思考", "解決策提案"],
        "model_name": "gpt-5",
        "temperature": 0.9,
        "provider": "openai"
      }
    ],
    "openrouter_config": {
      "model": "anthropic/claude-3-7-sonnet",
      "temperature": 0.5
    },
    "database": {
      "type": "json",
      "path": "./workspace/tasks.json"
    },
    "logging": {
      "level": "INFO",
      "file": "./logs/agent.log"
    },
    "tools": {
      "python_execute": {
        "enabled": true,
        "timeout": 30
      },
      "file_tool": {
        "enabled": true,
        "restricted_paths": []
      },
      "planning_tool": {
        "enabled": true,
        "max_steps": 10
      }
    },
    "error_handling": {
      "max_retry_attempts": 3,
      "error_correction": true
    }
  }
